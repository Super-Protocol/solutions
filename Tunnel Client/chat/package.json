{
  "name": "nextjs-chat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env APP_NODE_ENV=development NODE_ENV=development nodemon --experimental-specifier-resolution=node --esm server.ts",
    "start": "node ./dist/server.js",
    "lint": "next lint --fix",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "npm run build:server && cross-env APP_NODE_ENV=production npm run build:next",
    "install:build:docker": "docker build --platform linux/amd64 -f build.dockerfile -t chat-builder:latest . && docker run --platform linux/amd64 -it --rm -w /home/node -v ${PWD}:/home/node -v ~/.npmrc:/root/.npmrc chat-builder:latest bash -c 'npm ci && npm run build'"
  },
  "dependencies": {
    "@next/font": "^13.2.4",
    "@super-protocol/addons-tee": "^0.8.7",
    "@super-protocol/dto-js": "^0.0.28",
    "@super-protocol/sdk-js": "2.0.3",
    "@types/react": "18.0.29",
    "@types/react-dom": "18.0.11",
    "bcrypt": "^5.1.0",
    "bip39": "^3.1.0",
    "bootstrap": "^5.2.1",
    "classnames": "^2.3.2",
    "cookies-next": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "eslint": "8.36.0",
    "formik": "^2.2.9",
    "jsonwebtoken": "^9.0.0",
    "lru-cache": "^9.1.2",
    "mongoose": "^7.1.0",
    "net": "^1.0.2",
    "next": "13.4.9",
    "next-images": "^1.8.5",
    "nextjs-progressbar": "^0.0.16",
    "p-queue": "^6.6.2",
    "pino": "^8.11.0",
    "react": "18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "18.2.0",
    "react-input-mask": "^2.0.4",
    "react-query": "^3.39.3",
    "react-textarea-autosize": "^8.4.1",
    "react-toastify": "^9.1.3",
    "react-virtuoso": "^4.4.1",
    "sass": "^1.60.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "use-debounce": "^9.0.3",
    "uuid": "^9.0.0",
    "xss": "^1.0.14",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.10",
    "@types/react-input-mask": "^3.0.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.2"
  },
  "nodemonConfig": {
    "ignore": [
      "scripts/*"
    ]
  }
}
